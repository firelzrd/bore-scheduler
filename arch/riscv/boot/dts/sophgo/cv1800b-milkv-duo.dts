/dts-v1/;

#include "cv1800b.dtsi"

/ {
	model = "Milk-V Duo";
	compatible = "milkv,duo", "sophgo,cv1800b";

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x3f40000>;
	};
};

&osc {
	clock-frequency = <25000000>;
};

&uart0 {
	status = "okay";
};
